{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/test",
            "destination": "/api/test.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/client/assets/$1"
        },
        {
            "source": "/(.*)\\.(ico|png|svg|jpg|jpeg|css|js|json)",
            "destination": "/client/$1.$2"
        },
        {
            "source": "/(.*)",
            "destination": "/client/index.html"
        }
    ]
}